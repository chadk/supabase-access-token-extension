{
  "manifest_version": 3,
  "name": "Supabase Access Token Extractor",
  "version": "1.0",
  "description": "Extracts access_token from the Supabase sb-127-auth-token cookie",
  "permissions": ["cookies", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
